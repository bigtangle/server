<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane prefHeight="503.0" prefWidth="873.0"
	style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.bigtangle.ui.wallet.ExchangeController">
	<children>
		<AnchorPane prefHeight="416.0" prefWidth="567.0">
			<children>
				<TabPane layoutX="2.0" layoutY="-2.0" prefHeight="512.0"
					prefWidth="873.0">
					<tabs>
						<Tab text="%add">
							<content>
								<GridPane layoutX="2.0" layoutY="-9.0" prefHeight="483.0"
									prefWidth="676.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="246.0" minWidth="10.0" prefWidth="115.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="449.0" minWidth="10.0" prefWidth="424.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label text="%to_Address" />
										<Label text="%to_tokenid" GridPane.rowIndex="1" />
										<Label text="%to_tokenAmount" GridPane.rowIndex="2" />
										<Label text="%from_Address" GridPane.rowIndex="3" />
										<Label text="%from_Amount" GridPane.rowIndex="5" />
										<Label text="%from_Tokenid" GridPane.rowIndex="4" />
										<TextField fx:id="toAmountTextField"
											GridPane.columnIndex="1" GridPane.rowIndex="2" />
										<TextField fx:id="fromAmountTextField"
											GridPane.columnIndex="1" GridPane.rowIndex="5" />
										<Button mnemonicParsing="false" onAction="#closeUI"
											text="%close" GridPane.columnIndex="2" GridPane.rowIndex="6" />
										<GridPane GridPane.columnIndex="1" GridPane.rowIndex="6">
											<columnConstraints>
												<ColumnConstraints hgrow="SOMETIMES"
													maxWidth="145.0" minWidth="10.0" prefWidth="97.0" />
												<ColumnConstraints hgrow="SOMETIMES"
													maxWidth="229.0" minWidth="10.0" prefWidth="81.0" />
												<ColumnConstraints hgrow="SOMETIMES"
													maxWidth="274.0" minWidth="10.0" prefWidth="116.0" />
												<ColumnConstraints hgrow="SOMETIMES"
													maxWidth="274.0" minWidth="10.0" prefWidth="162.0" />
											</columnConstraints>
											<rowConstraints>
												<RowConstraints minHeight="10.0" prefHeight="30.0"
													vgrow="SOMETIMES" />
											</rowConstraints>
											<children>
												<Button mnemonicParsing="false" onAction="#exchangeCoin"
													text="%Exchange" />
												<Button mnemonicParsing="false" onAction="#importBlock"
													text="%import" GridPane.columnIndex="1" />
												<Button mnemonicParsing="false" onAction="#exportBlock"
													text="%export" GridPane.columnIndex="2" />
												<Button mnemonicParsing="false" onAction="#refund"
													text="%refund" GridPane.columnIndex="3" />
											</children>
										</GridPane>
										<ComboBox fx:id="toTokenHexComboBox" editable="true"
											prefHeight="23.0" prefWidth="420.0" GridPane.columnIndex="1"
											GridPane.rowIndex="1" />
										<ComboBox fx:id="fromTokenHexComboBox" editable="true"
											prefHeight="23.0" prefWidth="415.0" GridPane.columnIndex="1"
											GridPane.rowIndex="4" />
										<ComboBox fx:id="toAddressComboBox" editable="true"
											prefHeight="23.0" prefWidth="424.0" GridPane.columnIndex="1" />
										<ComboBox fx:id="fromAddressComboBox" editable="true"
											prefHeight="23.0" prefWidth="413.0" GridPane.columnIndex="1"
											GridPane.rowIndex="3" />
									</children>
								</GridPane>
							</content>
						</Tab>
						<Tab text="%search">
							<content>
								<AnchorPane minHeight="0.0" minWidth="0.0"
									prefHeight="180.0" prefWidth="200.0">
									<children>
										<TableView fx:id="exchangeTable" layoutX="14.0"
											layoutY="14.0" prefHeight="376.0" prefWidth="847.0">
											<columns>
												<TableColumn fx:id="orderidsCol" prefWidth="1.0"
													text="%orderid" visible="false" />
												<TableColumn fx:id="dataHexCol" prefWidth="1.0"
													text="%dataHex" visible="false" />
												<TableColumn fx:id="fromAddressCol" prefWidth="106.0"
													text="%from_Address" />
												<TableColumn fx:id="fromTokenidCol" prefWidth="145.0"
													text="%from_Tokenid" />
												<TableColumn fx:id="fromAmountCol" prefWidth="118.0"
													text="%from_Amount" />
												<TableColumn fx:id="toAddressCol" prefWidth="84.0"
													text="%to_Address" />
												<TableColumn fx:id="toTokenidCol" prefWidth="143.0"
													text="%to_tokenid" />
												<TableColumn fx:id="toAmountCol" prefWidth="54.0"
													text="%to_tokenAmount" />
												<TableColumn fx:id="toSignCol" prefWidth="54.0"
                                                    text="%to_sign" />
                                                <TableColumn fx:id="fromSignCol" prefWidth="54.0"
                                                    text="%from_sign" />
											</columns>
										</TableView>
										<Button layoutX="412.0" layoutY="426.0"
											mnemonicParsing="false" onAction="#closeUI" text="%close" />
										<Button layoutX="88.0" layoutY="426.0" mnemonicParsing="false"
											onAction="#signExchange" text="%sign" />
									</children>
								</AnchorPane>
							</content>
						</Tab>
					</tabs>
				</TabPane>
			</children>
		</AnchorPane>
	</children>
</StackPane>
