										<ComboBox fx:id="addressComboBox" editable="true" prefHeight="23.0" prefWidth="394.0" GridPane.columnIndex="1" />
										<ComboBox fx:id="tokenComboBox" editable="true" prefHeight="23.0" prefWidth="392.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
										<ChoiceBox fx:id="statusChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
										<DatePicker fx:id="validdateFromDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="5" />
										<DatePicker fx:id="validdateToDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="6" />
