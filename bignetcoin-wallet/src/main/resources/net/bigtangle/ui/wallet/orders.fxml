										<TableColumn fx:id="orderidCol" prefWidth="105.0" text="%orderid" />
										<TableColumn fx:id="addressCol" prefWidth="174.0" text="%address" />
										<TableColumn fx:id="tokenidCol" prefWidth="175.0" text="%token_id" />
