										<Label alignment="CENTER" prefHeight="15.0" prefWidth="308.0" text="%token" textAlignment="CENTER" GridPane.rowIndex="1" />
										<Label alignment="CENTER" prefHeight="15.0" prefWidth="254.0" text="%limit" GridPane.rowIndex="2" />
