										<Label alignment="CENTER" prefHeight="15.0" prefWidth="264.0" text="%Amount" GridPane.rowIndex="4" />
										<TextField fx:id="amountTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Label alignment="CENTER" prefHeight="15.0" prefWidth="266.0" text="%market" GridPane.rowIndex="3" />
                        <ComboBox fx:id="marketComboBox" editable="true" prefHeight="23.0" prefWidth="403.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
