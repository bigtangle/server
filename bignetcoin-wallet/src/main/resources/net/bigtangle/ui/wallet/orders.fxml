										<TableColumn fx:id="priceCol" prefWidth="85.0" text="%price" />
										<TableColumn fx:id="amountCol" prefWidth="101.0" text="%Amount" />
									</columns>
								</TableView>
								<Button layoutX="452.0" layoutY="478.0" mnemonicParsing="false" onAction="#closeUI" text="%close" />
								<Label layoutX="41.0" layoutY="66.0" text="%address" />
								<TextField fx:id="address4searchTextField" layoutX="98.0" layoutY="62.0" prefHeight="23.0" prefWidth="306.0" />
								<Label layoutX="413.0" layoutY="25.0" text="%state" />
