export SOURCE=/home/cui/bigdata/pn_discovery/gd_sparkserver
cd $SOURCE
sbt clean
sbt assembly
curl --data-binary @$SOURCE/target/scala-2.10/sparkjobserver-assembly-0.1.0-SNAPSHOT.jar localhost:8090/jars/test
curl -X DELETE 'localhost:8090/contexts/mytestcontext'
 
 sleep 5
curl -d "" 'localhost:8090/contexts/mytestcontext?context-factory=spark.jobserver.context.SQLContextFactory&sync=true'

curl -d "" 'localhost:8090/jobs?appName=test&classPath=de.gd.analytics.sparkjob.SparkSQLData&context=mytestcontext&sync=true'

