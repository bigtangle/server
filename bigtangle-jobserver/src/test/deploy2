export SOURCE=/home/cui/bigdata/pn_discovery/gd_sparkserver
cd $SOURCE
sbt clean
sbt   -DJAVATARGET=1.7 assembly
curl --data-binary @$SOURCE/target/scala-2.10/sparkjobserver-assembly-0.1.0-SNAPSHOT.jar 10.15.20.85:8090/jars/test
curl -X DELETE '10.15.20.85:8090/contexts/mytestcontext'
 
 sleep 5
curl -d "" '10.15.20.85:8090/contexts/mytestcontext?context-factory=spark.jobserver.context.SQLContextFactory&sync=true'

curl -d "" '10.15.20.85:8090/jobs?appName=test&classPath=de.gd.analytics.sparkjob.SparkSQLData&context=mytestcontext&sync=true&'

