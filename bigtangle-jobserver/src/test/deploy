export SOURCE=/home/cui/git/bignetcoin/bigtangle-jobserver
cd $SOURCE
sbt clean
sbt assembly
curl --data-binary @$SOURCE/target/scala-2.11/bigtangle-jobserver-assembly-0.1.0-SNAPSHOT.jar localhost:8090/jars/test
curl -X DELETE 'localhost:8090/contexts/mytestcontext'
 
 sleep 5
curl -d "" 'localhost:8090/contexts/mytestcontext?context-factory=spark.jobserver.context.SessionContextFactory&sync=true'

curl -d "" 'localhost:8090/jobs?appName=test&classPath=net.bigtangle.sparkjob.SparkSQLData&context=mytestcontext&sync=true'

